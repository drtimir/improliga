[
	["^/$", {
		"title":"Česká Improliga - divadlo jako hokejový zápas",
		"modules":[
			["system/partial", {
				"sections":["ui-home", "ui-about", "ui-shows", "ui-teams", "ui-workshops", "ui-media", "ui-contact"],
				"template":"public/viewport",
				"slot":"ui"
			}],
			["system/partial", {"template":"public/about"}],
			["system/partial", {"template":"public/contacts"}],
			["system/partial", {"template":"public/knowledge"}],
			["article/category/list"]
		]
	}, "home"],

	["^/tymy$", {"modules":[["team/list"]]}, "teams"],
	["^/predstaveni$", {"modules":[
		["system/partial", {"template":"public/viewport", "slot":"ui", "sections":["ui-shows"]}],
		["seo/event/archive", {
			"conds":{
				"0":"type in (1,2,3,4,6)",
				"published":true
			}
		}]
	]}, "events"],
	["^/workshopy$", {"modules":[["seo/event/calendar"]]}, "workshops"],
	["^/o-improlize$", {"title":"O Improlize", "modules":[["system/partial", {"template":"public/knowledge"}]]}, "about"],
	["^/media-o-improlize$", {"title":"impro_menu_media", "layout":["layout/public"], "modules":[["system/partial", {"template":"impro/static/media"}]]}, "media"],
	["^/kontakty$", {"title":"Kontakty - Česká Improliga", "modules":[["system/partial", {"template":"public/contacts"}]]}, "contacts"],

	["^/predstaveni/mesic/([^/]+)$", {
		"modules":[
			["seo/event/month", {
				"month":"#{0}",
				"conds":{
					"published":true,
					"type":[1,2,3,4,6]
				}
			}]
		]
	}, "shows_month"],

	["^/predstaveni/([^/]+)$", {
		"modules":[
			["system/partial", {"template":"public/viewport", "slot":"ui", "sections":["ui-event"]}],
			["seo/event/detail", {
				"id":"#{0}",
				"conds":{
					"published":true,
					"type":[1,2,3,4,6]
				}
			}]
		]
	}, "show_detail"],


	["^/workshopy/mesic/([^/]+)$", {
		"modules":[
			["event/list/month", {
				"month":"#{0}",
				"conds":{
					"published":true,
					"type":5
				}
			}]
		]
	}, "workshops_month"],

	["^/workshopy/([^/]+)$", {
		"modules":[
			["event/list/archive", {
				"conds":{
					"published":true,
					"type":5
				}
			}]
		]
	}, "workshop_detail"],


	["^/o-improlize/([^/]+)/$", {
		"layout":["layout/public", "layout/public/dual-34"],
		"modules":[
			["article/detail", {"id":"#{0}", "slot":"left"}],
			["article/category/list", {"slot":"right"}]
		]
	}, "article"],

	["^/o-improlize/([^/]+)/#([^/]+)$", {}, "article_chapter"],


	["^/tymy/([^/]+)$", {
		"layout":["layout/public"],
		"modules":[
			["impro/team/detail", {"id":"#{0}"}]
		]
	}, "team"],

	["^/intranet/$", {
		"modules":[
			["system/redirect", {"url":"http://intra.improliga.cz", "code":301}]
		]
	}, "intranet_redir"]
]
