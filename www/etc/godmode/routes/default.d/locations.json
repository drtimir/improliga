{
	"#":{
		"title":"godmode_location_list",
		"template":["pwf/godmode/window"],
		"modules":[
			["godmode/location/list"]
		]
	},

	"create":{"#":{
		"title":"godmode_location_create",
		"modules":[
			["godmode/location/edit", {"new":true}]
		]
	}},

	"*":{"#":{
			"title":"godmode_location",
			"modules":[
				["godmode/location/detail", {"id":"#{0}"}]
			]
		},

		"edit":{"#":{
			"title":"godmode_location_edit",
			"modules":[
				["godmode/location/edit", {"id":"#{0}"}]
			]
		}},

		"delete":{"#":{
			"title":"godmode_location_delete",
			"modules":[
				["godmode/location/delete", {"id":"#{0}"}]
			]
		}}
	}
}
