[
	["^/$", {
		"title":"Intranet Improligy"
	}, "home"],

	["^/((tymy|kontakty|predstaveni|workshopy|diskuze|sdilene).*)?$", {
		"title":"Intranet Improligy"
	}, "frontend"],


	["^/service/invite/([^/]+)$", {
		"layout":["layout/intra/service"],
		"modules":[
			["team/member/register", {
				"auth_uid":"#{0}"
			}]
		]
	}, "team_member_register"],


	["^/api/team/([^/]+)/member/invite$", {
		"modules":[["team/member/invite", {"id_team":"#{0}"}]],
		"format":"json"
	}, "team_member_invite"],


	["^/user/status/$", {
			"modules":[
				["impro/user/status"]
			]
		}, "user_status"],

	["^/user/status/detail.json/$", {
			"modules":[
				["impro/user/status/detail", {
					"format":"json"
				}]
			]
		}, "user_status"],

	["^/login$", {
		"title":"Přihlášení",
		"layout":["layout/intra/login"],
		"modules":[
			["impro/user/login"],
			["system/partial", {"template":"login/gateways"}]
		]
	}, "login"],

	["^/login/facebook$", {
		"layout":["layout/intra/login"],
		"init":"facebook",
		"modules":[["impro/user/login_facebook"]]
	}, "login_facebook"],

	["^/logout$", {
		"modules":[
			["impro/user/logout", {"redirect":"/"}]
		]
	}, "logout"],


	["^/podpora$", {
	}, "support"],

	["^/podminky$", {
	}, "terms"]
]
