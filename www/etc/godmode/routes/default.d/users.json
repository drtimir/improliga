{
	"#":{
		"title":"godmode_users",
		"template":["godmode/window"],
		"modules":[
			["godmode/user/list"]
		]
	},

	"create":{"#":{
		"title":"godmode_user_create",
		"modules":[
			["godmode/user/edit", {"new":true}]
		]
	}},


	"groups":{"#":{
			"title":"godmode_user_group",
			"modules":[
				["godmode/user/group_list"]
			]
		},

		"create":{"#":{
			"title":"godmode_user_group_create",
			"modules":[
				["godmode/user/group_edit", {"new":true}]
			]
		}},

		"*":{"#":{
				"title":"godmode_user_group_members",
				"modules":[
					["godmode/user/group_members", {"group_id":"#{0}"}]
				]
			},

			"edit":{"#":{
				"title":"godmode_user_group_edit",
				"modules":[
					["godmode/user/group_edit", {"id":"#{0}"}]
				]
			}},

			"delete":{"#":{
				"title":"godmode_user_group_delete",
				"modules":[
					["godmode/user/group_delete", {"id":"#{0}"}]
				]
			}}
		}
	},

	"perms":{"#":{
			"title":"godmode_user_perms_list",
			"modules":[
				["godmode/user/perm_list"]
			]
		},

		"*":{"#":{
				"title":"title_godmode_user_perms_module",
				"modules":[
					["godmode/user/perm_module", {"mod_path":"#{0}"}]
				]
			},
			"*":{"#":{
				"title":"title_godmode_user_perms_module_switch",
				"modules":[
					["godmode/user/perm_module", {"mod_path":"#{0}", "group_id":"#{1}"}]
				]
			}}
		}
	},

	"*":{"#":{
			"title":"godmode_user_detail",
			"modules":[
				["godmode/user/detail", {"id":"#{0}"}]
			]
		},

		"edit":{"#":{
			"title":"godmode_user_edit",
			"modules":[
				["godmode/user/edit", {"id":"#{0}"}]
			]
		}},

		"edit_groups":{"#":{
			"title":"godmode_user_edit_groups_assignment",
			"modules":[
				["godmode/user/edit_groups", {"id":"#{0}"}]
			]
		}},

		"edit-contacts":{"#":{
			"title":"godmode_user_edit_contacts",
			"modules":[
				["godmode/user/edit_contacts", {"id":"#{0}"}]
			]
		}},

		"delete":{"#":{
			"title":"godmode_delete",
			"modules":[
				["godmode/user/delete", {"id":"#{0}"}]
			]
		}}
	}
}
