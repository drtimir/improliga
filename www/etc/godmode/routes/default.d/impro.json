{
	"#":{
		"template":["godmode/window"]
	},

	"events":{
		"#":{
			"title":"impro_events",
			"modules":[
				["godmode/impro/event/list"]
			]
		},

		"create":{
			"#":{
				"title":"impro_event_create",
				"modules":[
					["godmode/impro/event/edit", {"new":true}]
				]
			}
		},

		"*":{"#":{
				"title":"event_detail"
			}

		}
	},

	"teams":{
		"#":{
			"title":"impro_teams",
			"modules":[
				["godmode/impro/team/list"]
			]
		},

		"create":{
			"#":{
				"title":"impro_team_create",
				"modules":[
					["godmode/impro/team/edit", {"new":true}]
				]
			}
		},

		"*":{"#":{
				"title":"impro_team_detail",
				"modules":[
					["godmode/impro/team/detail", {"id":"#{0}"}]
				]
			},

			"add_member":{"#":{
				"title":"impro_team_member_add",
				"modules":[
					["godmode/impro/team/member/add", {"team_id":"#{0}"}]
				]
			}},

			"members":{
				"#":{
					"title":"impro_team_members_list",
					"modules":[
						["godmode/impro/team/member/list", {"team_id":"#{0}"}]
					]
				},

				"*":{
					"#":{
						"title":"impro_team_member_detal",
						"modules":[
							["godmode/impro/team/member/detail", {"team_id":"#{0}", "id":"#{1}"}]
						]
					},

					"edit":{"#":{
						"title":"impro_team_member_edit",
						"modules":[
							["godmode/impro/team/member/edit", {"team_id":"#{0}", "id":"#{1}"}]
						]
					}},

					"delete":{"#":{
						"title":"impro_team_member_delete",
						"modules":[
							["godmode/impro/team/member/delete", {"team_id":"#{0}", "id":"#{1}"}]
						]
					}}
				}
			},

			"edit":{
				"#":{
					"title":"impro_team_edit",
					"modules":[
						["godmode/impro/team/edit", {"id":"#{0}"}]
					]
				}
			}
		}
	}
}
