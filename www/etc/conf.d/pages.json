{
	"#":{
		"title":"impro_name",
		"template":["impro/layout", "impro/homepage"],
		"modules":[
			["system/text/detail", {"slot":"left", "id":1}],
			["impro/event/latest", {
				"slot":"events",
				"link_cont":"/udalosti/{seoname}/",
				"link_team":"/tymy/{seoname}/",
				"conds":{"published":true},
				"thumb_width":216,
				"thumb_height":165,
				"per_page":4
			}],
			["impro/event/calendar", {
				"slot":"right",
				"day_link_integrate":true,
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"heading":"Kalendář",
				"conds":{"published":true}
			}]
		]
	},


	"o-improlize":{
		"#":{
			"title":"impro_menu_about",
			"template":["impro/layout", "impro/dual_34_14"],
			"modules":[
				["system/text/detail", {"id":2, "slot":"left"}],
				["impro/article/category/list", {"slot":"right"}]
			]
		},

		"fauly":{
			"#":{
				"title":"Fauly",
				"modules":[
					["impro/foul/list", {"slot":"right"}]
				]
			},

			"*":{
				"#":{
					"modules":[
						["impro/foul/detail", {"id":"#{0}", "slot":"left"}],
						["impro/foul/list", {"slot":"right"}]
					]
				}
			}
		},

		"*":{
			"#":{
				"title":"impro_article",
				"modules":[
					["impro/article/detail", {"id":"#{0}", "slot":"left"}],
					["impro/article/category/list", {"slot":"right"}]
				]
			}
		}
	},


	"media-o-nas":{"#":{
		"title":"impro_menu_media",
		"template":["impro/layout", "impro/single"],
		"modules":[
			["system/partial", {"template":"impro/static/media"}]
		]
	}},


	"tymy":{"#":
		{
			"title":"impro_menu_teams",
			"template":["impro/layout", "impro/single"],
			"modules":[
				["impro/team/list"]
			]
		},

		"*":{
			"#":{
				"modules":[
					["impro/team/detail", {"id":"#{0}"}]
				]
			}
		}
	},


	"novinky":{
		"#":{
			"title":"impro_menu_news",
			"template":["impro/layout", "impro/dual"],
			"modules":[
				["impro/news/list", {"slot":"events_right", "link_cont":"/novinky/{seoname}/", "conds":{"published":true}}]
			]
		},

		"*":{"#":{
			"modules":{
				"detail":["impro/news/detail", {"id":"#{0}", "slot":"left", "link_cont":"/novinky/{seoname}/", "conds":{"published":true}}],
				"list":["impro/news/list", {"slot":"right", "link_cont":"/novinky/{seoname}/", "conds":{"published":true}}, ["detail"]]
			}
		}}
	},


	"kontakty":{"#":{
		"title":"impro_contacts",
		"template":["impro/layout", "impro/dual"],
		"modules":[
			["system/text/detail", {"id":4, "slot":"left"}],
			["system/text/detail", {"id":3, "slot":"right"}]
		]
	}},


	"udalosti":{
		"#":{
			"title":"impro_events",
			"template":["impro/layout", "impro/dual"],
			"modules":[
				["impro/event/calendar", {
					"slot":"left",
					"template":"impro/event/list_month",
					"link_cont":"/udalosti/{seoname}/",
					"link_month":"/udalosti/seznam/{year}-{month}/",
					"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
					"link_day_piece":"den_{day}"
				}],
				["impro/event/calendar", {
					"slot":"right",
					"day_link_integrate":true,
					"link_month":"/udalosti/seznam/{year}-{month}/",
					"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
					"link_day_piece":"den_{day}",
					"heading":"Kalendář",
					"conds":{"published":true}
				}]
			]
		},

		"*":{"#":{
			"template":["impro/layout", "impro/single"],
			"modules":[
				["impro/event/detail", {
					"id":"#{0}",
					"col_width":440,
					"template":"impro/event/detail_short",
					"link_team":"/tymy/{seoname}/",
					"link_event":"/udalosti/{seoname}/"
				}]
			]
		}},

		"seznam":{
			"#":{
				"title":"impro_events",
				"modules":[
					["impro/event/latest", {
						"slot":"events_left",
						"link_cont":"/udalosti/{seoname}/",
						"link_team":"/tymy/{seoname}/",
						"link_day_piece":"den_{day}",
						"conds":{"published":true}
					}],
					["impro/event/calendar", {
						"slot":"events_right",
						"day_link_integrate":true,
						"link_month":"/udalosti/seznam/{year}-{month}/",
						"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
						"link_day_piece":"den_{day}",
						"heading":"Kalendář",
						"conds":{"published":true}
					}]
				]
			},

			"*":{
				"#":{
					"modules":[
						["impro/event/calendar", {
							"slot":"right",
							"day_link_integrate":true,
							"link_month":"/udalosti/seznam/{year}-{month}/",
							"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
							"link_day_piece":"den_{day}",
							"month_input":"#{0}"
						}],
						["impro/event/calendar", {
							"slot":"left",
							"template":"impro/event/list_month",
							"link_cont":"/udalosti/{seoname}/",
							"link_month":"/udalosti/seznam/{year}-{month}/",
							"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
							"link_day_piece":"den_{day}",
							"month_input":"#{0}"
						}]
					]
				}
			}
		}
	},

	"intranet":{
		"#":{
			"modules":[
				["system/redirect", {"url":"http://intra.improliga.cz", "code":301}]
			]
		}
	},

	"intra":{
		"#":{
			"template":["intra/layout", "intra/dual"],
			"title":"Intranet Improligy",
			"modules":{
				"news":["impro/news/list", {"slot":"left"}],
				"cl":["impro/event/calendar", {"slot":"right", "day_link_integrate":true, "heading":"Kalendář"}, ["cc"]]
			}
		},

		"login":{"#":{
			"title":"Přihlášení",
			"template":["intra/login"],
			"modules":[["intra/user/login"]]
		}},

		"logout":{"#":{
			"modules":[
				["intra/user/logout", {"redirect":"/"}]
			]
		}},

		"news":{
			"#":{},
			"*":{
				"#":{
					"modules":[
						["impro/news/list", {"slot":"right"}],
						["godmode/impro/news/detail", {"id":"#{0}", "slot":"left"}]
					]
				}
			}
		},

		"teams":{
			"#":{},
			"*":{
				"#":{
					"title":"Můj tým",
					"template":["intra/layout", "intra/dual_34_14"],
					"modules":[

					]
				}
			}
		},

		"profile":{
			"#":{
				"title":"Můj profil",
				"modules":[
					["impro/user/profile", {"self":true, "slot":"left"}]
				]
			},

			"events":{"#":{
				"title":"intra_profile_my_events",
				"modules":[
					["impro/event/my", {"slot":"left", "month_input":"#{0}"}]
				]
			}},

			"*":{
				"#":{
					"modules":[
						["impro/user/profile", {"id":"#{0}", "slot":"left"}]
					]
				}
			}
		},

		"discussion":{"#":{
			"title":"Diskuze",
			"template":["intra/layout", "intra/dual_34_14"],
			"modules":[
				["impro/discussion/board_list", {"conds":{"public":true}, "slot":"left"}]
			]},

			"create_board":{"#":{
				"title":"Vytvořit nový obor témat",
				"modules":[
					["impro/discussion/board_edit", {"new":true, "slot":"left"}]
				]
			}},

			"*":{"#":{
				"title":"Seznam témat",
				"modules":[
					["impro/discussion/topic_list", {"id_board":"#{0}", "slot":"left"}]
				]},

				"edit":{"#":{
					"title":"Upravit obor",
					"modules":[
						["impro/discussion/board_edit", {"id":"#{0}", "slot":"left"}]
					]
				}},

				"delete":{"#":{

				}},

				"create_topic":{"#":{
					"title":"Vytvoření tématu",
					"modules":[
						["impro/discussion/topic_edit", {"id_board":"#{0}", "new":true, "slot":"left"}]
					]
				}},

				"*":{"#":{
					"modules":[
						["impro/discussion/post_add",  {"id_board":"#{0}", "id_topic":"#{1}", "slot":"discussion_post_form"}],
						["impro/discussion/post_list", {"id_board":"#{0}", "id":"#{1}", "slot":"left"}]
					]},

					"edit":{"#":{
						"title":"Upravit téma",
						"modules":[
							["impro/discussion/topic_edit", {"id_board":"#{0}", "id":"#{1}", "slot":"left"}]
						]
					}},

					"create_post":{"#":{
						"title":"Vytvoření příspěvku",
						"modules":[
							["impro/discussion/post_edit", {"new":true, "id_board":"#{0}", "id_topic":"#{1}", "slot":"left"}]
						]
					}}
				}
			}
		},

		"events":{
			"#":{
				"title":"intra_events",
				"template":["intra/layout", "intra/dual_34_14"],
				"modules":[
					["impro/event/calendar", {"slot":"left", "template":"impro/event/list_month", "link_cont":"/events/{id_impro_event}/"}],
					["impro/event/calendar", {"slot":"right", "day_link_integrate":true, "day_names_short":true}]
				]
			},

			"*":{
				"#":{
					"title":"intra_event",
					"template":["intra/layout", "intra/single"],
					"modules":[
						["impro/event/detail", {"id":"#{0}"}]
					]
				},
				"edit":{
					"#":{
						"template":["intra/layout", "intra/dual_34_14"],
						"title":"intra_create_event",
						"modules":{
							"step":["impro/event/wizzard/name", {"slot":"left", "id":"#{0}", "link_wizzard":"/events/{id_impro_event}/edit/{step}/"}],
							"status":["impro/event/wizzard/status", {"slot":"right", "link_wizzard":"/events/{id_impro_event}/edit/{step}/"}, ["step"]]
						}
					},

					"cancel":{"#":{
						"title":"intra_create_event",
						"modules":[
							["impro/event/wizzard/cancel", {"slot":"left", "id":"#{0}", "link_wizzard":"/events/{id_impro_event}/edit/{step}/"}]
						]
					}},

					"*":{"#":{
						"title":"intra_create_event",
						"modules":{
							"step":["impro/event/wizzard/step", {"slot":"left", "id":"#{0}", "step":"#{1}", "link_wizzard":"/events/{id_impro_event}/edit/{step}/"}],
							"status":["impro/event/wizzard/status", {"slot":"right", "link_wizzard":"/events/{id_impro_event}/edit/{step}/"}, ["step"]]
						}
					}}
				}
			},

			"list":{"#":{},
				"*":{"#":{
					"title":"intra_events",
					"modules":[
						["impro/event/calendar", {"slot":"right", "day_link_integrate":true, "day_names_short":true, "month_input":"#{0}"}],
						["impro/event/calendar", {"slot":"left", "template":"impro/event/list_month", "link_cont":"/events/{id_impro_event}/", "month_input":"#{0}"}]
					]
				}}
			},

			"create":{
				"#":{
					"title":"intra_create_event",
					"modules":{
						"step":["impro/event/wizzard/name", {"slot":"left", "new":true}],
						"status":["impro/event/wizzard/status", {"slot":"right"}, ["step"]]
					}
				},
				"cancel":{"#":{
					"title":"intra_create_event",
					"modules":[
						["impro/event/wizzard/cancel", {"slot":"left", "new":true}]
					]
				}},
				"*":{"#":{
					"title":"intra_create_event",
					"modules":{
						"step":["impro/event/wizzard/step", {"step":"#{0}", "slot":"left", "new":true}],
						"status":["impro/event/wizzard/status", {"slot":"right"}, ["step"]]
					}
				}}
			}
		},


		"settings":{
			"#":{
				"template":["intra/layout", "intra/single"],
				"modules":[
					["intra/user/settings"]
				]
			}
		}
	}
