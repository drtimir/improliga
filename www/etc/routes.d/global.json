[
	["^/$", {
		"title":"impro_name",
		"template":["impro/layout", "impro/homepage"],
		"modules":[
			["system/text/detail", {"slot":"left", "id":1}],
			["impro/event/latest", {
				"slot":"events",
				"link_cont":"/udalosti/{seoname}/",
				"link_team":"/tymy/{seoname}/",
				"conds":{"published":true},
				"thumb_width":216,
				"thumb_height":165,
				"per_page":4
			}],
			["impro/event/calendar", {
				"slot":"right",
				"day_link_integrate":true,
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"heading":"Kalendář",
				"conds":{"published":true}
			}]
		]
	}, "public_home"],


	["^/o-improlize/$", {
		"title":"impro_menu_about",
		"template":["impro/layout", "impro/dual_34_14"],
		"modules":[
			["system/text/detail", {"id":2, "slot":"left"}],
			["impro/article/category/list", {"slot":"right"}]
		]
	}, "public_about"],

	["^/fauly/$", {
		"title":"Fauly",
		"modules":[
			["impro/foul/list", {"slot":"right"}]
		]
	}, "public_fauls"],

	["^/fauly/([^/]+)/$", {
		"modules":[
			["impro/foul/detail", {"id":"#{1}", "slot":"left"}],
			["impro/foul/list", {"slot":"right"}]
		]
	}, "public_foul_detail"],

	["^/o-improlize/([^/]+)/$", {
		"title":"impro_article",
		"modules":[
			["impro/article/detail", {"id":"#{1}", "slot":"left"}],
			["impro/article/category/list", {"slot":"right"}]
		]
	}, "public_article"],

	["^/media-o-nas/$", {
		"title":"impro_menu_media",
		"template":["impro/layout", "impro/single"],
		"modules":[
			["system/partial", {"template":"impro/static/media"}]
		]
	}, "public_media"],

	["^/tymy/$", {
		"title":"impro_menu_teams",
		"template":["impro/layout", "impro/single"],
		"modules":[
			["impro/team/list"]
		]
	}, "public_teams"],

	["^/tymy/([^/]+)/$", {
		"modules":[
			["impro/team/detail", {"id":"#{1}"}]
		]
	}, "public_team_detail"],

	["^/kontakty/$", {
		"title":"impro_contacts",
		"template":["impro/layout", "impro/dual"],
		"modules":[
			["system/text/detail", {"id":4, "slot":"left"}],
			["system/text/detail", {"id":3, "slot":"right"}]
		]
	}, "public_contacts"],

	["^/udalosti/$", {
		"title":"impro_events",
		"template":["impro/layout", "impro/dual"],
		"modules":[
			["impro/event/calendar", {
				"slot":"left",
				"template":"impro/event/list_month",
				"link_cont":"/udalosti/{seoname}/",
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"link_day_piece":"den_{day}"
			}],
			["impro/event/calendar", {
				"slot":"right",
				"day_link_integrate":true,
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"link_day_piece":"den_{day}",
				"heading":"Kalendář",
				"conds":{"published":true}
			}]
		]
	}, "public_events"],

	["^/udalosti/([^/]+)/$", {
		"template":["impro/layout", "impro/single"],
		"modules":[
			["impro/event/detail", {
				"id":"#{1}",
				"col_width":440,
				"template":"impro/event/detail_short",
				"link_team":"/tymy/{seoname}/",
				"link_event":"/udalosti/{seoname}/"
			}]
		]
	}, "public_event_detail"],

	["^/udalosti/seznam/$", {
		"title":"impro_events",
		"modules":[
			["impro/event/latest", {
				"slot":"events_left",
				"link_cont":"/udalosti/{seoname}/",
				"link_team":"/tymy/{seoname}/",
				"link_day_piece":"den_{day}",
				"conds":{"published":true}
			}],
			["impro/event/calendar", {
				"slot":"events_right",
				"day_link_integrate":true,
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"link_day_piece":"den_{day}",
				"heading":"Kalendář",
				"conds":{"published":true}
			}]
		]
	}, "public_event_list"],

	["^/udalosti/seznam/([^/]+)/$", {
		"modules":[
			["impro/event/calendar", {
				"slot":"right",
				"day_link_integrate":true,
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"link_day_piece":"den_{day}",
				"month_input":"#{1}"
			}],
			["impro/event/calendar", {
				"slot":"left",
				"template":"impro/event/list_month",
				"link_cont":"/udalosti/{seoname}/",
				"link_month":"/udalosti/seznam/{year}-{month}/",
				"link_day":"/udalosti/seznam/{year}-{month}/#den_{day}",
				"link_day_piece":"den_{day}",
				"month_input":"#{1}"
			}]
		]
	}, "public_event_month"],

	["^/intranet/$", {
		"modules":[
			["system/redirect", {"url":"http://intra.improliga.cz", "code":301}]
		]
	}, "public_intranet_redir"]
]
